Let c as 2x2 matrix
c = [ 
    1, 2;
    1, 2
    ]

Let b be [ 
    1, 2;
    1, 2
    ] as matrix

Print("\nMatrix decleration:\nC =\n" + c + ",\n\nb =\n" + b + "\n\n")

Print("\nMatrix addition C + B =\n" + (c + b) + "\n\n")

c[0, 1] = 3

Print("\nC[0, 1] = 3\n\n" + "C =\n" + c + "\n")

Print("\nMatrix multiplication C * B =\n" + (c * b))

Print("\n-----------------------------------------------------------------------------\n\n")